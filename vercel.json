{
  "env": {
    "GROQ_API_KEY": "@groq-api-key"
  },
  "functions": {
    "api/stage-generator/*.js": {
      "maxDuration": 10
    },
    "api/stage-controller.js": {
      "maxDuration": 10
    },
    "api/polling-generator.js": {
      "maxDuration": 10
    },
    "api/free-plan-generator.js": {
      "maxDuration": 10
    },
    "api/integrated-micro-generator.js": {
      "maxDuration": 10
    },
    "api/env-check.js": {
      "maxDuration": 10
    },
    "api/health.js": {
      "maxDuration": 10
    },
    "api/validate-api-key.js": {
      "maxDuration": 10
    },
    "api/export.js": {
      "maxDuration": 10
    },
    "api/scenario-storage.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/generate-stage",
      "destination": "/api/stage-controller"
    }
  ]
}